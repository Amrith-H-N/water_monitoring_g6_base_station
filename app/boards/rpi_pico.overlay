
&uart1 {
current-speed = <115200>;
status = "okay";
pinctrl-0 = <&uart1_default>;
pinctrl-names = "default";
		
	water: water {
		compatible = "zephyr,water";
		status = "disabled";
		zephyr,deferred-init;
		
};
};



&uart0 {
    status = "okay";
    current-speed = <115200>;
};

&i2c0 {
	status = "disabled";
};

&spi0{
	status = "disabled";
};